<Window x:Class="Para.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ui="clr-namespace:Para.UI"
        xmlns:control="clr-namespace:Para.UI.Control"
        xmlns:text="clr-namespace:Para.UI.Text"
        xmlns:local="clr-namespace:Para"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="Black">
        <control:Caret Height="202" Width="4" Background="White"/>
        <text:SpriteChar Char="x" FontSize="30" Width="6" Height="12" Foreground="White"/>
        <control:TextBox Height="30" Width="600"/>
    </Grid>
</Window>
